<?xmlversion="1.0"encoding="UTF-8"?>
<HIARequestOrderData xmlns="urn:org:ebics:H004"
xmlns:ds="http://www.w3.org/2000/09/xmldsig#"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="urn:org:ebics:H004 ebics_keymgmt_request_H004.xsd">
    <AuthenticationPubKeyInfo>
        <PubKeyValue>
            <ds:RSAKeyValue>
                <ds:Modulus><%= @user.key('x002').public_modulus %></ds:Modulus>
                <ds:Exponent><%= @user.key('x002').public_exponent %></ds:Exponent>
            </ds:RSAKeyValue>
            <TimeStamp><%= @user.key('x002').created_at %></TimeStamp>
        </PubKeyValue>
        <AuthenticationVersion>X002</AuthenticationVersion>
    </AuthenticationPubKeyInfo>

    <EncryptionPubKeyInfo>
        <PubKeyValue>
            <ds:RSAKeyValue>
                <ds:Modulus><%= @user.key('e002').public_modulus %></ds:Modulus>
                <ds:Exponent><%= @user.key('e002').public_exponent %></ds:Exponent>
            </ds:RSAKeyValue>
            <TimeStamp><%= @user.key('e002').created_at %></TimeStamp>
        </PubKeyValue>
        <EncryptionVersion>E002</EncryptionVersion>
    </EncryptionPubKeyInfo>

    <PartnerID><%= @user.partner_id %></PartnerID>
    <UserID><%= @user.member_id %></UserID>
</HIARequestOrderData>
